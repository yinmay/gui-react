{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "trailing-comma": false,
        "no-console": false,
        "interface-over-type-literal": false,
    },
    "rulesDirectory": []
}